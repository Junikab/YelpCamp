    <body>
        <%- include("partials/navBar.ejs") %> 


        <h1>Your random number is: <%=rand %></h1>
        <% if (rand % 2===0){ %> 
        <h2>This is an even number!</h2>
        <% } else {%>
            <h2>This is an odd number!</h2>
        <% } %> 
        <p>Here is an other way to do it:</p>
        
        <h3>This number is an <%=rand%2===0 ? "even" : "odd"  %> number. </h3>
    </body>
